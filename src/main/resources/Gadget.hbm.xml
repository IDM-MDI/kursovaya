<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="by.ishangulyev.kursovaya.entity.Gadget" table="gadget" schema="kurs">
        <id name="id">
            <column name="id" sql-type="int"/>
            <generator class="increment"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="cpuId">
            <column name="cpuID" sql-type="int" not-null="true"/>
        </property>
        <property name="memoryId">
            <column name="memoryID" sql-type="int" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="int" not-null="true"/>
        </property>
        <property name="batteryId">
            <column name="batteryID" sql-type="int" not-null="true"/>
        </property>
        <property name="videoId">
            <column name="videoID" sql-type="int" not-null="true"/>
        </property>
        <property name="audioId">
            <column name="audioID" sql-type="int" not-null="true"/>
        </property>
        <property name="categoryId">
            <column name="categoryID" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>